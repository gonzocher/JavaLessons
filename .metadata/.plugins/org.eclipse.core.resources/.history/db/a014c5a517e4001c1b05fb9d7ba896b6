
public class WinTicTacToe {

	public static void main(String[] args) {
		// Tic Tac Toe game in the console
		gameBoard();
		

	}
	
	public static void gameBoard() {
		
	// Game Board (2D array)
		String[][] gameBoard = {
							{"  ", "|", "  ", "|", "  "},
							{"--", "+", "--", "+", "--"},
							{"  ", "|", "  ", "|", "  "},
							{"--", "+", "--", "+", "--"},
							{"  ", "|", "  ", "|", "  "},
							};
		
		for(String[] row : gameBoard) {
			for(String c : row) {
				System.out.print(c);
			}
			System.out.println();
		}
	}

}
