import java.util.HashMap;

public class HashTheMap {

	public static void main(String[] args) {

		int a = 10;
		int b = 3;
		int c = 88;
	
		HashMap<String, Integer> temps = new HashMap<String, Integer>();
		temps.put("a", 10);
		temps.put("b", 3);
		temps.put("c", 88);
		
		System.out.println(temps);
		System.out.println("----------");
		System.out.println(temps.get("c"));
	}

}
