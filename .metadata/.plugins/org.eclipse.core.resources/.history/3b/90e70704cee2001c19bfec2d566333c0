
public class Return {

	public static void main(String[] args) {
		printAMessage();
		add(5,4);
		}
		// Returns occur inside a method
		
	

	public static void printAMessage() {
		System.out.println("This is our first method!");
	}
	
	public static void add(int a, int b) {
		System.out.println(a + b);
	}
}
