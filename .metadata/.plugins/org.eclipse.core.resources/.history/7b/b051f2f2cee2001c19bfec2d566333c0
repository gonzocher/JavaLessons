
public class Return {

	public static void main(String[] args) {
		printAMessage();
		System.out.println(add(5,4));
		}
		// Returns occur inside a method
		
	

	public static void printAMessage() {
		System.out.println("This is our first method!");
	}
	
	public static int add(int a, int b) {
		return (a + b);
	}
}
